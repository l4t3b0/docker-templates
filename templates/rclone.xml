<?xml version="1.0"?>
<Container version="2">
  <Name>rclone</Name>
  <Repository>pfidr/rclone</Repository>
  <Registry>https://hub.docker.com/r/pfidr/rclone/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://</Support>
  <Project>https://rclone.org/</Project>
  <Overview>Rclone is a command line program to sync files and directories to and from cloud provider</Overview>
  <Category>Cloud:</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/l4t3b0/docker-templates/master/templates/rclone.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/l4t3b0/docker-templates/master/templates/icons/rclone.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Rclone is a command line program to sync files and directories to and from cloud provider</Description>
  <Labels/>
  <Config Name="Port" Target="53682" Default="" Mode="tcp" Description="Port" Type="Port" Display="always" Required="true" Mask="false">53682</Config>
  <Config Name="Host Key 1" Target="SYNC_SRC" Default="" Mode="" Description="Container Variable: SYNC_SRC" Type="Variable" Display="advanced-hide" Required="false" Mask="false">99</Config>
</Container>
